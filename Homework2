let age;
let name;
let hobbies;


let user =new Object();
user.name='Joseph';
user.age= 30;
user.hobbies =['singing','reading','basketball','football']

user.lenght=function(){
  return hobbies.lenght;
}


if(age>user.length)
  console.log("true");

else if(age ==user.lenght){
  console.log("false");
}
  else
  console.log("undefined");
